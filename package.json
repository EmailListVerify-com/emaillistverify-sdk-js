{
  "name": "emaillistverify-sdk",
  "version": "1.0.0",
  "description": "TypeScript/JavaScript SDK for EmailListVerify API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts examples/**/*.ts",
    "lint:fix": "eslint src/**/*.ts examples/**/*.ts --fix",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/EmailListVerify-com/emaillistverify-sdk-js"
  },
  "keywords": [
    "email",
