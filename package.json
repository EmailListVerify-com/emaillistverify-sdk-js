{
  "name": "@devcyberpanda/emaillistverify-sdk",
  "version": "0.1.0",
  "description": "Official TypeScript SDK wrapper for the EmailListVerify API.",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": ["dist", "README.md", "LICENSE"],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "prepublishOnly": "npm run build",
    "test": "vitest run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EmailListVerify-com/emaillistverify-sdk-js.git"
  },
  "bugs": {
    "url": "https://github.com/EmailListVerify-com/emaillistverify-sdk-js/issues"
  },
  "homepage": "https://github.com/EmailListVerify-com/emaillistverify-sdk-js#readme",
  "keywords": [
    "email",
    "validation",
    "verification",
    "email-validation",
    "email-verification",
    "emaillistverify",
    "api",
    "sdk",
    "typescript"
  ],
  "author": "EmailListVerify",
  "license": "MIT",
  "publishConfig": { "access": "public" },
  "engines": { "node": ">=18" },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "axios": "^1.12.0"
  }
}