    "email-verification",
    "email-validation",
    "bulk-verification"
  ],
  "author": "EmailListVerify <support@emaillistverify.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EmailListVerify-com/emaillistverify-sdk-js/issues"
  },
  "homepage": "https://github.com/EmailListVerify-com/emaillistverify-sdk-js#readme",
  "dependencies": {
    "axios": "^1.4.0",
    "form-data": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.3.0",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "eslint": "^8.42.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.1.3"
  },
  "engines": {
    "node": ">=14.0.0"
  }
